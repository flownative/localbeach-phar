{
    "name": "flownative/beach-cli-distribution",
    "description": "Beach CLI Distribution",
    "license": "proprietary",
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "ext-json": "*",

        "symfony/config": "^4.0",
        "symfony/console": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "padraic/phar-updater": "^1.0",
        "neos/utility-arrays": "^5.0",
        "neos/utility-files": "^5.0",
        "google/cloud-storage": "^1.4",
        "guzzlehttp/psr7": "^1.4",
        "vlucas/phpdotenv": "^2.5"
    },
    "suggest": {
        "ext-posix": "For more reliable interactive terminal detection"
    },
    "autoload": {
        "files": ["constants.php"],
        "psr-4": {
            "Flownative\\Beach\\Cli\\": "src/Flownative/Beach/Cli"
        }
    }
}
